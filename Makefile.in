# $Id: Makefile.in,v 1.1 1997-10-14 05:54:31 ghudson Exp $

SHELL=/bin/sh
VPATH=@srcdir@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

CC=@CC@
DEFS=@DEFS@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
ALL_CFLAGS=${CPPFLAGS} ${CFLAGS} ${DEFS}

all: listsuidcells

listsuidcells: listsuidcells.o
	${CC} ${LDFLAGS} -o listsuidcells listsuidcells.o -lsys -lrx -llwp \
		-lsys ${LIBS}

.c.o:
	${CC} -c ${ALL_CFLAGS} $<

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${bindir}
	${INSTALL_PROGRAM} listsuidcells ${DESTDIR}${bindir}

clean:
	rm -f listsuidcells.o listsuidcells

distclean: clean
	rm -f config.cache config.log config.status Makefile
